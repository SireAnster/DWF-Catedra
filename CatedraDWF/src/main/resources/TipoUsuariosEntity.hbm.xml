<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.com.catedra.www.catedradwf.entities.TipoUsuariosEntity" table="tipo_usuarios" schema="dwf_catedra">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="tipoDeUsuario">
            <column name="Tipo de usuario" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>